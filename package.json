{
  "name": "linkedin-scraper",
  "version": "1.0.0",
  "description": "LinkedIn Job Scraper built with NestJS",
  "private": true,
  "scripts": {
    "prebuild": "npx puppeteer browsers install chrome",
    "build": "nest build",
